
public class symArr {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int zahl = 1;
		int arr[][] = new int [4][3];
		
		for(int i = 0; i < arr.length; i++)
		{
			arr[i]= new int [zahl];
			System.out.println();
			for(int j = 0; j < arr[i].length; j++)
			{
				arr[i][j] = zahl;
				System.out.print(arr[i][j]);
			}
			zahl++;
		}
		
	}

}
